@prefix void: <http://rdfs.org/ns/void#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix brum: <http://faculty.washington.edu/tgis/ld/brumfield/uwDataset/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix uwrecon: <http://faculty.washington.edu/tgis/ld/brumfield/uwDataset/vocabularies/reconciliation#> .

<> a void:DatasetDescription ;
	dcterms:title "Description of the 'rights' dataset" ;
	void:inDataset <http://faculty.washington.edu/tgis/ld/brumfield/uwDataset> ;
      foaf:primaryTopic brum:rights.rdf ;
      dcterms:subject brum:rights.rdf .
brum:rights.rdf a void:Dataset ;
		dcterms:title "William Craft Brumfield Russian Architecture Digital Collection RDF Data rights dataset" ;
      dcterms:description "This dataset lists all known rights statements relevant to the 'works' dataset and all the 'photos' datasets in the William Craft Brumfield Russian Architecture Digital Collection RDF Data." ;
      dcterms:hasFormat brum:rights.ttl, brum:rights.nt ;
      uwrecon:deDuped true ;
      uwrecon:containsEntity dcterms:RightsStatement ;
      void:inDataset <http://faculty.washington.edu/tgis/ld/brumfield/uwDataset> ;
      void:dataDump brum:rights.rdf, brum:rights.ttl, brum:rights.nt .
